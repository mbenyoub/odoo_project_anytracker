<?xml version="1.0" encoding="utf-8"?>
<openerp>
  <data noupdate="1">
    <!-- gtd method -->
    <record model="anytracker.priority" id="priority_gtd_asap">
      <field name="active" eval="1"/>
      <field name="method_id" ref="method_gtd"/>
      <field name="name">ASAP</field>
      <field name="seq" eval="20"/>
    </record>
    <record model="anytracker.priority" id="priority_gtd_today">
      <field name="active" eval="1"/>
      <field name="method_id" ref="method_gtd"/>
      <field name="name">Today</field>
      <field name="seq" eval="40"/>
    </record>
    <record model="anytracker.priority" id="priority_gtd_week">
      <field name="active" eval="1"/>
      <field name="method_id" ref="method_gtd"/>
      <field name="name">This week</field>
      <field name="seq" eval="60"/>
    </record>
    <record model="anytracker.priority" id="priority_gtd_month">
      <field name="active" eval="1"/>
      <field name="method_id" ref="method_gtd"/>
      <field name="name">This month</field>
      <field name="seq" eval="60"/>
    </record>
    <record model="anytracker.priority" id="priority_gtd_if_time">
      <field name="active" eval="1"/>
      <field name="method_id" ref="method_gtd"/>
      <field name="name">Maybe one day</field>
      <field name="seq" eval="80"/>
    </record>

    <!-- maintenance -->
    <record model="anytracker.priority" id="priority_maintenance_asap">
      <field name="active" eval="1"/>
      <field name="method_id" ref="method_maintenance"/>
      <field name="name">ASAP!!!</field>
      <field name="seq" eval="10"/>
    </record>
    <record model="anytracker.priority" id="priority_maintenance_deadline">
      <field name="active" eval="1"/>
      <field name="method_id" ref="method_maintenance"/>
      <field name="name">Before deadline</field>
      <field name="deadline" eval="1"/>
      <field name="seq" eval="20"/>
    </record>
    <record model="anytracker.priority" id="priority_maintenance_next">
      <field name="active" eval="1"/>
      <field name="method_id" ref="method_maintenance"/>
      <field name="name">Next to do</field>
      <field name="seq" eval="30"/>
    </record>
    <record model="anytracker.priority" id="priority_maintenance_nodeadline">
      <field name="active" eval="1"/>
      <field name="method_id" ref="method_maintenance"/>
      <field name="name">No deadline</field>
      <field name="seq" eval="30"/>
    </record>


    <!-- Implementation -->
    <record model="anytracker.priority" id="priority_implementation_asap">
      <field name="name">ASAP!!!</field>
      <field name="description">As soon as possible</field>
      <field name="seq" eval="10"/>
      <field name="active" eval="1"/>
      <field name="method_id" ref="method_implementation"/>
    </record>
    <record model="anytracker.priority" id="priority_implementation_deadline">
      <field name="name">Before deadline</field>
      <field name="deadline" eval="1"/>
      <field name="seq" eval="20"/>
      <field name="active" eval="1"/>
      <field name="method_id" ref="method_implementation"/>
    </record>
    <record model="anytracker.priority" id="priority_implementation_milestone">
      <field name="name">Before milestone</field>
      <field name="deadline" eval="1"/>
      <field name="seq" eval="30"/>
      <field name="active" eval="1"/>
      <field name="method_id" ref="method_implementation"/>
    </record>
    <record model="anytracker.priority" id="priority_implementation_next">
      <field name="name">Next to do</field>
      <field name="seq" eval="40"/>
      <field name="active" eval="1"/>
      <field name="method_id" ref="method_implementation"/>
    </record>
    <record model="anytracker.priority" id="priority_implementation_nodeadline">
      <field name="active" eval="1"/>
      <field name="method_id" ref="method_implementation"/>
      <field name="name">No deadline</field>
      <field name="seq" eval="30"/>
    </record>

  </data>
</openerp>

