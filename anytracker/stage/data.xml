            <field name="name">To do</field>
            <field name="state">todo</field>
            <field name="method_id" ref="method_simple"/>
            <field name="sequence">10</field>
            <field name="progress">0.0</field>
        </record>
        <record id="stage_simple_doing" model="anytracker.stage">
            <field name="name">In progress</field>
            <field name="state">doing</field>
            <field name="method_id" ref="method_simple"/>
            <field name="sequence">20</field>
            <field name="progress">50.0</field>
        </record>
        <record id="stage_simple_done" model="anytracker.stage">
            <field name="name">Done</field>
            <field name="state">done</field>
            <field name="method_id" ref="method_simple"/>
            <field name="sequence">30</field>
            <field name="progress">100.0</field>
        </record>
    </data>
</openerp>
