<?xml version="1.0" encoding="utf-8"?>
<openerp>
    <data>
    <!-- Procédé pour ticket forfait -->
    
    <record id="process_process_ticket_forfait0" model="process.process">
        <field eval="1" name="active"/>
        <field name="name">"Process ticket forfait"</field>
    </record>

    <!-- étapes du procédé -->

    <record id="process_node_ticket_forfait_analyse" model="process.node">
 <field eval="&quot;&quot;&quot;Analyse&quot;&quot;&quot;" name="name"/>        
    <field name="kind" eval="&quot;&quot;&quot;state&quot;&quot;&quot;"/>
        <field name="process_id" ref="process_process_ticket_forfait0"/>
        <field name="model_states" eval="&quot;&quot;&quot;object.state_dev in 
        ('draft', 'open', 'pending', 'done', 'cancel')&quot;&quot;&quot;"/>
        <field eval="1" name="flow_start"/>
    </record>
    
    <record id="process_node_ticket_forfait_dev" model="process.node">
 <field eval="&quot;&quot;&quot;Dev&quot;&quot;&quot;" name="name"/>
<field name="kind" eval="&quot;&quot;&quot;state&quot;&quot;&quot;"/>
        <field name="process_id" ref="process_process_ticket_forfait0"/>
        <field name="model_states" eval="&quot;&quot;&quot;object.state_dev in 
        ('draft', 'open', 'pending', 'done', 'cancel')&quot;&quot;&quot;"/>
        <field name="flow_start" eval="0"/>    
    </record>

    <record id="process_node_ticket_forfait_test" model="process.node">
 <field eval="&quot;&quot;&quot;Test&quot;&quot;&quot;" name="name"/>
<field name="kind" eval="&quot;&quot;&quot;state&quot;&quot;&quot;"/>        
<field name="process_id" ref="process_process_ticket_forfait0"/>
        <field name="model_states" eval="&quot;&quot;&quot;object.state_dev in 
