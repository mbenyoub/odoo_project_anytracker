    </record>
    
    <record id="process_node_ticket_forfait_abandon" model="process.node">
<field name="kind" eval="&quot;&quot;&quot;state&quot;&quot;&quot;"/>
 <field eval="&quot;&quot;&quot;Abandon&quot;&quot;&quot;" name="name"/>
        <field name="process_id" ref="process_process_ticket_forfait0"/>
        <field name="model_states" eval="&quot;&quot;&quot;object.state_dev in 
        ('cancel', 'done')&quot;&quot;&quot;"/>
        <field eval="0" name="flow_start"/>
    </record>
    
   
    <!--
    transitions possibles
    attention a l'ordre target_node_id et source_node_id
    -->
    
    <record id="process_transition_ticket_forfait_analyse_dev" 
            model="process.transition">
        <field name="name">nom</field>
        <field name="transition_id" eval="[(6,0,[])]"/>
        <field name="target_node_id" model="process.node" 
               ref="process_node_ticket_forfait_dev"/>
        <field name="source_node_id" model="process.node" 
               ref="process_node_ticket_forfait_analyse"/>
    </record>

