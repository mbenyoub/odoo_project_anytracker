      <field name="seq" eval="20"/>
    </record>
    <record model="anytracker.importance" id="importance_maintenance_detail">
      <field name="active" eval="1"/>
      <field name="method_id" ref="method_maintenance"/>
      <field name="name">Detail</field>
      <field name="seq" eval="10"/>
    </record>

    <!-- Scrum -->
    <record model="anytracker.importance" id="importance_scrum_must">
      <field name="name">Must have</field>
      <field name="description">Critical to project success and has to be included in the current delivery timebox in order for it to be a success.</field>
      <field name="seq" eval="20"/>
      <field name="active" eval="1"/>
      <field name="method_id" ref="method_scrum"/>
    </record>
    <record model="anytracker.importance" id="importance_scrum_should">
      <field name="active" eval="1"/>
      <field name="method_id" ref="method_scrum"/>
      <field name="name">Should have</field>
      <field name="description">Important to project success, but not necessary for the next delivery.</field>
      <field name="seq" eval="20"/>
    </record>
    <record model="anytracker.importance" id="importance_scrum_could">
      <field name="active" eval="1"/>
      <field name="method_id" ref="method_scrum"/>
      <field name="name">Could have</field>
      <field name="description"> Not critical but would be nice to have if it does not cost too much.</field>
      <field name="seq" eval="20"/>
    </record>
    <record model="anytracker.importance" id="importance_scrum_wont">
      <field name="active" eval="1"/>
      <field name="method_id" ref="method_scrum"/>
      <field name="name">Won't have</field>
      <field name="description">Not planned for this version of the application, but may be reconsidered for later version.</field>
      <field name="seq" eval="10"/>
    </record>

    <!-- Implementation -->
    <record model="anytracker.importance" id="importance_implementation_must">
      <field name="name">Must have</field>
