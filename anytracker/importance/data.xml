<?xml version="1.0" encoding="utf-8"?>
<openerp>
  <data noupdate="1">
    <!-- gtd method -->
    <record model="anytracker.importance" id="importance_gtd_notimportant">
      <field name="active" eval="1"/>
      <field name="method_id" ref="method_gtd"/>
      <field name="name">Not important</field>
      <field name="seq" eval="100"/>
    </record>
    <record model="anytracker.importance" id="importance_gtd_important">
      <field name="active" eval="1"/>
      <field name="method_id" ref="method_gtd"/>
      <field name="name">Important</field>
      <field name="seq" eval="20"/>
    </record>
    <record model="anytracker.importance" id="importance_gtd_wishlist">
      <field name="active" eval="1"/>
      <field name="method_id" ref="method_gtd"/>
      <field name="name">Future wishlist</field>
      <field name="seq" eval="10"/>
    </record>

    <!-- maintenance -->
    <record model="anytracker.importance" id="importance_maintenance_blocker">
      <field name="active" eval="1"/>
      <field name="method_id" ref="method_maintenance"/>
      <field name="name">Blocker</field>
      <field name="seq" eval="50"/>
    </record>
    <record model="anytracker.importance" id="importance_maintenance_critical">
      <field name="active" eval="1"/>
      <field name="method_id" ref="method_maintenance"/>
      <field name="name">Critical</field>
      <field name="seq" eval="40"/>
    </record>
    <record model="anytracker.importance" id="importance_maintenance_major">
      <field name="active" eval="1"/>
      <field name="method_id" ref="method_maintenance"/>
